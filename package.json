    "bugs": {
        "url": "https://github.com/Eltik/image-proxy/issues"
    },
    "homepage": "https://github.com/Eltik/image-proxy#readme",
    "dependencies": {
        "axios": "^0.27.2",
        "body-parser": "^1.20.2",
        "cheerio": "^1.0.0-rc.12",
        "colors": "^1.4.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-domain-middleware": "^0.1.0"
    }
}
